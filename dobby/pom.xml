<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.riscure</groupId>
    <artifactId>dobby-all</artifactId>
    <version>0.1.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <repositories>
        <repository>
            <id>riscure</id>
            <url>http://nexus3.riscure.com:8081/repository/riscure</url>
            <snapshots><enabled>false</enabled></snapshots>
        </repository>
        <repository>
            <id>riscure-snapshots</id>
            <url>http://nexus3.riscure.com:8081/repository/riscure-snapshots</url>
            <snapshots><enabled>true</enabled></snapshots>
        </repository>
        <repository>
            <id>3rdparty</id>
            <url>http://nexus3.riscure.com:8081/repository/3rdparty</url>
            <snapshots><enabled>false</enabled></snapshots>
        </repository>
        <repository>
            <id>3rdparty-snapshots</id>
            <url>http://nexus3.riscure.com:8081/repository/3rdparty-snapshots</url>
            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </snapshots>
        </repository>
        <repository>
            <id>p2-proxy</id>
            <url>http://nexus3.riscure.com:8081/repository/p2-proxy</url>
            <snapshots><enabled>true</enabled></snapshots>
        </repository>
    </repositories>
    <modules>
        <module>shell-parser</module>
        <module>dobby</module>
    </modules>
</project>
